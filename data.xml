<inputs>
	<field type="Document level">
		<name>ponumber</name><!-- internal label for the scraper script ONLY -->
		<trigger type='raw'>PO Number</trigger><!-- label for this field on the excel -->
		<trigger type='raw'>P/O No</trigger><!-- alternate labels -->
		<trigger type='raw'>po number</trigger>
		<cell>1, 0</cell><!-- where is the actual data associated with this field? provide (x, y) coordinates--> 
		<action type="Direct Output">PO Number</action><!-- no processing needed, just scrape the cell and output directly -->
	</field> 
	<field type="Document level">
		<name>issuedate</name>
		<trigger type='raw'>Date</trigger>
		<cell>1, 0</cell>
		<action type="Direct Output">PO Date</action>
	</field> 
	<field type="Document level">
		<name>deliverydate</name>
		<trigger type='raw'>Delivery</trigger>
		<cell>1, 0</cell>
		<action type="Direct Output">Delivery Date</action>
	</field> 
	<field type="Document level">
		<name>project</name>
		<trigger type='raw'>PROJECT</trigger>
		<cell>1, 0</cell>
		<action type="Direct Output">Project Site</action>
	</field> 
	<field type="Document level">
		<name>note</name>
		<trigger type='raw'>**Note</trigger>
		<cell>1, 0</cell>
		<cell>2, 0</cell>
		<action type="Direct Output">Note Raw</action>
	</field> 
	<field type="Document level">
		<name>detail</name>
		<trigger type='regex'>^DETAIL.+</trigger>
		<cell>0, 0</cell>
		<cell>1, 0</cell>
		<action type="Direct Output">Note Raw</action>
	</field> 
	<field type="Document level">
		<name>galvanised</name>
		<trigger type='raw'>Galvanised</trigger>
		<cell>1, 0</cell>
		<action type="Direct Output">Galvanised</action>
	</field> 
	<field type="Document level">
		<name>urgentreason</name>
		<trigger type='raw'>Reason</trigger>
		<cell>1, 0</cell>
		<action type="Direct Output">Reason</action>
	</field> 
	<field type="Document level">
		<name>urgentreason</name>
		<trigger type='raw'>Reason</trigger>
		<cell>1, 0</cell>
		<action type="Direct Output">Reason</action>
	</field> 
	<field type="Table">
		<name>maintable</name>
		<trigger type='raw'>ITEM</trigger>
		<trigger type='raw'>item</trigger>
		<trigger type='raw'>Item</trigger>
		<action type="Parse">Main Table</action>
	</field> 
</inputs>
