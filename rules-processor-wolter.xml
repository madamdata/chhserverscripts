		<rule type="coerceDate">
			<name>coerceIssueDate</name>
			<input>PO Date</input>
		</rule>
		<rule type="coerceDate">
			<name>coerceDeliveryDate</name>
			<input>Delivery Date</input>
		</rule>
		<rule type="coerceDate">
